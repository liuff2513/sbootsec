create table sys_user (
    "id" int4 not null,
    "username" varchar(255),
    "password" varchar(255),
    primary key ("id")
) with (oids=false);

create table sys_role (
 "id" int4 not null,
 "name" varchar(255),
 primary key ("id")
) with (oids=false);

create table sys_user_roles (
 "sys_user_id" int4 not null,
 "roles_id" int4 not null,
 primary key ("sys_user_id","roles_id")
) with (oids=false);

